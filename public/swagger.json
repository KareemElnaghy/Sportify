{
    "openapi": "3.0.0",
    "info": {
        "title": "Sportify",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:3000"
        }
    ],
    "paths": {
        "/adminAccounts/list": {
            "get": {
                "tags": [
                    "admins"
                ],
                "summary": "getAdminsList",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "schema": {
                            "type": "integer"
                        },
                        "description": "used for pagination"
                    },
                    {
                        "name": "recordsPerPage",
                        "in": "query",
                        "schema": {
                            "type": "integer"
                        },
                        "description": "number of records to include in each page"
                    },
                    {
                        "name": "searchQuery",
                        "in": "query",
                        "schema": {
                            "type": "string"
                        },
                        "description": "text string used for filtering"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Admin"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/adminAccounts": {
            "put": {
                "tags": [
                    "admins"
                ],
                "summary": "updateAdminAccount",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "firstName": {
                                        "type": "string",
                                        "example": "John"
                                    },
                                    "lastName": {
                                        "type": "string",
                                        "example": "Doe"
                                    }
                                },
                                "additionalProperties": false
                            },
                            "example": {
                                "firstName": "John",
                                "lastName": "Doe"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Admin"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "admins"
                ],
                "summary": "createAdminAccount",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Admin"
                            },
                            "example": {
                                "email": "admin@aucegypt.edu",
                                "firstName": "John",
                                "lastName": "Doe"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "example": {
                                    "message": "Successful Response",
                                    "admin": {
                                        "email": "admin@example.com",
                                        "firstName": "John",
                                        "lastName": "Doe"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "admins"
                ],
                "summary": "deleteAdminAccount",
                "requestBody": {
                    "description": "List of emails to delete",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "emails": {
                                        "type": "array",
                                        "items": {
                                            "type": "string",
                                            "format": "email"
                                        }
                                    }
                                },
                                "required": [
                                    "emails"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "string",
                                    "enum": [
                                        "Success",
                                        "Fail"
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        },
        "/courts/list": {
            "get": {
                "tags": [
                    "courts"
                ],
                "summary": "getCourtsList",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "schema": {
                            "type": "integer"
                        },
                        "description": "used for pagination"
                    },
                    {
                        "name": "recordsPerPage",
                        "in": "query",
                        "schema": {
                            "type": "integer"
                        },
                        "description": "number of records to include in each page"
                    },
                    {
                        "name": "searchQuery",
                        "in": "query",
                        "schema": {
                            "type": "string"
                        },
                        "description": "text string used for filtering"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/Court"
                                        },
                                        {
                                            "type": "integer"
                                        }
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        },
        "/courts": {
            "get": {
                "tags": [
                    "courts"
                ],
                "summary": "getCourts",
                "parameters": [
                    {
                        "name": "courtIDs",
                        "in": "query",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            },
                            "maxItems": 1000
                        },
                        "description": "An array of IDs unique to the courts (max 1000)"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Court"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "courts"
                ],
                "summary": "createCourt",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "description": "Name of the court"
                                    },
                                    "sport": {
                                        "type": "string",
                                        "description": "Dedicated Sport"
                                    },
                                    "location": {
                                        "type": "string",
                                        "description": "Location of the court "
                                    },
                                    "description": {
                                        "type": "string",
                                        "description": "Description of the court (optional)"
                                    }
                                },
                                "required": [
                                    "name",
                                    "sport"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Court"
                                }
                            }
                        }
                    }
                }
            },
            "put": {
                "tags": [
                    "courts"
                ],
                "summary": "updateCourt",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "description": "Updated name of the court"
                                    },
                                    "location": {
                                        "type": "string",
                                        "description": "Updated location of the court"
                                    },
                                    "description": {
                                        "type": "string",
                                        "description": "Updated description of the court"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Court"
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "courts"
                ],
                "summary": "deleteCourt",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "description": "An array of courtIDs to delete"
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "string",
                                    "enum": [
                                        "Success",
                                        "Fail"
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        },
        "/partyPosts/list": {
            "get": {
                "tags": [
                    "partyPost"
                ],
                "summary": "getPartyPostList",
                "parameters": [
                    {
                        "name": "page",
