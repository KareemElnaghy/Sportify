                        "schema": {
                            "type": "integer"
                        },
                        "description": "used for pagination"
                    },
                    {
                        "name": "recordsPerPage",
                        "in": "query",
                        "schema": {
                            "type": "integer"
                        },
                        "description": "number of records to include in each page"
                    },
                    {
                        "name": "searchQuery",
                        "in": "query",
                        "schema": {
                            "type": "string"
                        },
                        "description": "text string used for filtering"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/PartyPost"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/partyPosts": {
            "get": {
                "tags": [
                    "partyPost"
                ],
                "summary": "getPartyPosts",
                "parameters": [
                    {
                        "name": "id",
                        "in": "query",
                        "schema": {
                            "type": "string"
                        },
                        "description": "list of IDs"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/PartyPost"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "partyPost"
                ],
                "summary": "createPartyPost",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/PartyPost"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/PartyPost"
                                }
                            }
                        }
                    }
                }
            },
            "put": {
                "tags": [
                    "partyPost"
                ],
                "summary": "updatePartyPost",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/PartyPost",
                                "example": {}
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/PartyPost"
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "partyPost"
                ],
                "summary": "deletePartyPost",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                },
                                "example": [
                                    "id1",
                                    "id2",
                                    "id3"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "string",
                                    "enum": [
                                        "Success",
                                        "Fail"
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        },
        "/students/list": {
            "get": {
                "tags": [
                    "students"
                ],
                "summary": "getStudentsList",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "schema": {
                            "type": "integer"
                        },
                        "description": "used for pagination"
                    },
                    {
                        "name": "recordsPerPage",
                        "in": "query",
                        "schema": {
                            "type": "integer"
                        },
                        "description": "number of records per page"
                    },
                    {
                        "name": "searchQuery",
                        "in": "query",
                        "schema": {
                            "type": "string"
                        },
                        "description": "text string used for filtering"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Student"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/students": {
            "get": {
                "tags": [
                    "students"
                ],
                "summary": "getStudents",
                "parameters": [
                    {
                        "name": "emails",
                        "in": "query",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string",
                                "format": "email"
                            }
                        },
                        "description": "Array of student emails"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Student"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "put": {
                "tags": [
                    "students"
                ],
                "summary": "updateStudent",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Student"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Student"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Admin": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "firstName": {
                        "type": "string"
                    },
                    "lastName": {
                        "type": "string"
                    }
                }
            },
            "Court": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "id": {
                        "type": "integer"
                    },
                    "sport": {
                        "type": "string"
                    },
                    "location": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    }
                }
            },
            "PartyPost": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "ownerEmail": {
                        "type": "string"
                    },
                    "member": {
                        "type": "string"
                    },
                    "eventName": {
                        "type": "string"
                    },
                    "sport": {
                        "type": "string"
                    },
                    "location": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "startTime": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "endTime": {
                        "type": "string",
                        "format": "date-time"
                    }
                }
            },
            "Student": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "firstName": {
                        "type": "string"
                    },
                    "lastName": {
                        "type": "string"
                    },
                    "photoLink": {
                        "type": "string",
                        "nullable": true
                    },
                    "isTrainer": {
                        "type": "boolean"
                    },
                    "phoneNumber": {
                        "type": "string"
                    },
                    "isBanned": {
                        "type": "boolean"
                    }
                }
            }
        }
    }
}